{
  "name": "solid-frontend",
  "version": "1.0.0",
  "description": "Solid frontend for FastAPI",
  "main": "index.js",
  "keywords": [
    "solid",
    "frontend",
    "fastapi",
    "spa",
    "rest-client"
  ],
  "author": "Santiago",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen && npm run generate:types",
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "prettier .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check": "npm run lint && npm run format",
    "check:fix": "npm run lint:fix && npm run format:fix",
    "generate:types": "openapi-typescript http://localhost:8000/api/v1/openapi.json -o src/services/api/types.ts",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/solid": "^5.21.0",
    "@tanstack/solid-form": "^1.19.3",
    "@tanstack/solid-query": "^5.87.4",
    "@tanstack/solid-query-devtools": "^5.87.4",
    "@tanstack/solid-router": "^1.131.28",
    "@tanstack/solid-router-devtools": "^1.131.28",
    "@tanstack/solid-table": "^8.21.3",
    "lucide-solid": "^0.542.0",
    "openapi-fetch": "^0.14.0",
    "solid-js": "^1.9.9",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@pandacss/dev": "^1.2.0",
    "@park-ui/panda-preset": "^0.43.1",
    "@tanstack/router-plugin": "^1.131.31",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "openapi-typescript": "^7.9.1",
    "prettier": "3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.11.8"
  }
}
